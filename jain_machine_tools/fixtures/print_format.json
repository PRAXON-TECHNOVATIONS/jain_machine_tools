[
 {
  "absolute_value": 0,
  "align_labels_right": 0,
  "css": "\r\nth{\r\n    color:#111!important;\r\n    font-weight:500!important;\r\n}\r\n.print-format td, .print-format th {\r\n    padding: 3px !important;\r\n}\r\n.tab_label{\r\n    font-size:11px!important;\r\n    vertical-align:baseline!important;\r\n    font-weight:bold;\r\n    \r\n}\r\n.print-format th{\r\n    border-bottom:1px solid #111!important;\r\n}\r\n\r\n.print-format td{\r\n    vertical-align:baseline!important;\r\n}\r\n.item_tab thead th,.tax_tab thead th{\r\n    font-weight:bold!important;\r\n    text-align:center;\r\n    vertical-align:middle!important;\r\n}\r\n.bank_info th{\r\n    font-size:12px!important;  \r\n    cont-weight:bold\r\n}\r\n\r\n\r\n\r\n\r\n@page {\r\n    size: A4;\r\n    margin: 3mm;\r\n}\r\n\r\n\r\n.top-fixed-header {\r\n    position: fixed;\r\n    top: 1mm;\r\n    left: 6mm;\r\n    right: 6mm;\r\n}\r\n\r\n.top-fixed-header .left {\r\n    float: left;\r\n    font-weight: bold;\r\n}\r\n\r\n.top-fixed-header .right {\r\n    float: right;\r\n    font-weight: bold;\r\n}\r\n\r\n.print-format {\r\n    padding-top: 7mm !important;\r\n}\r\n.bottom-fixed-footer {\r\n    position: fixed;\r\n    bottom: 0mm;\r\n    left: 0mm;\r\n    right: 0mm;\r\n    text-align: center;\r\n    font-size: 10px;\r\n}\r\n\r\n.page:before {\r\n    content: counter(page);\r\n}\r\n.topage:before {\r\n    content: counter(pages);\r\n}\r\n\r\n\r\n.items-table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n}\r\n\r\n.items-table thead {\r\n    display: table-header-group;\r\n}\r\n\r\n.items-table tbody tr {\r\n    page-break-inside: auto;\r\n}\r\n\r\n.fixed-footer-image {\r\n    position: fixed;\r\n    bottom: 6mm;\r\n    left: 3mm;\r\n    right: 3mm;\r\n}\r\n\r\n.fixed-footer-image img {\r\n    width: 100%;\r\n    height: 50px;\r\n}\r\n.po-watermark {\r\n    position: fixed;\r\n    top: 30%;\r\n    left: 0;\r\n    width: 100%;\r\n    text-align: center;\r\n    font-size: 100px;\r\n    font-weight: bold;\r\n    color: rgba(200,0,0,0.1); /* Redish transparent color */\r\n    transform: rotate(-30deg);\r\n    z-index: 9999;\r\n    pointer-events: none; /* Print me interfere na kare */\r\n}",
  "custom_format": 1,
  "default_print_language": "en",
  "disabled": 0,
  "doc_type": "Purchase Order",
  "docstatus": 0,
  "doctype": "Print Format",
  "font": null,
  "font_size": 14,
  "format_data": null,
  "html": "<div class=\"top-fixed-header\">\r\n    <div class=\"left\">\r\n        {{ doc.name }}\r\n    </div>\r\n    <div class=\"right\">\r\n        {{ frappe.utils.formatdate(doc.transaction_date, \"dd-MMM-yyyy\") }}\r\n    </div>\r\n</div>\r\n\r\n<div style=\"border:2px solid #111;color:#000!important;\">\r\n    <table style=\"width:100%;font-family: 'Helvetica Neue', Arial, sans-serif;border:1px solid #111!important;\">\r\n        <table style=\"width: 100%; font-family: 'Helvetica Neue', Arial, sans-serif; border-bottom: 1px solid #111;\">\r\n            <tr>\r\n                <td style=\"width: 100%; vertical-align: top;\">\r\n                    <img src=\"/files/JMT HEADER.png\" style=\"width: 100%; height: 130px;\">\r\n                </td>\r\n            </tr>\r\n        </table>\r\n        <div style=\"text-align:center;background-color:#b6e1fb;\">\r\n    <span style=\"font-weight: bold!important;\">\r\n        PURCHASE ORDER\r\n        {% if doc.workflow_state != \"Approved\" and doc.workflow_state != \"Rejected\"%}\r\n            (NOT APPROVED)\r\n        {% endif %}\r\n        {% if doc.workflow_state == \"Rejected\" %}\r\n            (REJECTED)\r\n        {% endif %}\r\n    </span>\r\n</div>\r\n\r\n            <!-- Header Table -->\r\n            <!-- Outer container table -->\r\n        <table style=\"width:100%; border-collapse:collapse; font-family: Arial, sans-serif; font-size:10px;\">\r\n                        <!-- Main row with 3 columns: Vendor | Consignee | PO Info -->\r\n            <tr>\r\n                        <!-- LEFT: Vendor (50% of available) -->\r\n                <td style=\"width:35%; vertical-align:top; border:1px solid #000;\">\r\n                    <table style=\"width:100%; border-collapse:collapse;font-size:10px\">\r\n                        <tr>\r\n                            <td style=\"font-weight:bold; font-size:12px;\">VENDOR DETAILS</td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td><strong>Company Name:</strong> {{ doc.supplier_name or \"\" }}</td>\r\n                        </tr>\r\n                        <!--<tr>-->\r\n                        <!--    <td><strong>Kind Attention:</strong> {{ doc.contact_display or \"\" }}</td>-->\r\n                        <!--</tr>-->\r\n                        <tr>\r\n    <td>\r\n        <strong>Address:</strong><br/>\r\n        {% if doc.supplier_address %}\r\n            {% set addr = frappe.get_doc(\"Address\", doc.supplier_address) %}\r\n            {{ addr.address_line1 or \"\" }}<br/>\r\n            {% if addr.address_line2 %}{{ addr.address_line2 }}<br/>{% endif %}\r\n            {{ addr.city or \"\" }}<br/>\r\n            {{ addr.state or \"\" }}{% if addr.pincode %} - {{ addr.pincode }}{% endif %}<br/>\r\n            {{ addr.country or \"\" }}\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n\r\n                        <tr>\r\n                            <td><strong>Contact No:</strong> {{ doc.contact_mobile or \"\" }}</td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td><strong>GST:</strong> {{ doc.supplier_gstin or \"\" }}</td>\r\n                        </tr>\r\n                    </table>\r\n                </td>\r\n\r\n                <!-- MIDDLE: Consignee (35%) -->\r\n                <td style=\"width:35%; vertical-align:top; border-top:1px solid #000; border-bottom:1px solid #000; border-right:1px solid #000;\">\r\n                    <table style=\"width:100%; border-collapse:collapse;font-size:10px\">\r\n                        <tr>\r\n                            <td style=\"font-weight:bold; font-size:12px;\">CONSIGNEE DETAILS</td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td><strong>Name:</strong> {{ doc.company or \"\" }}</td>\r\n                        </tr>\r\n                        <tr>\r\n    <td>\r\n        <strong>Address:</strong><br/>\r\n        {% if doc.shipping_address %}\r\n            {% set addr = frappe.get_doc(\"Address\", doc.shipping_address) %}\r\n            {{ addr.address_line1 or \"\" }}<br/>\r\n            {% if addr.address_line2 %}{{ addr.address_line2 }}<br/>{% endif %}\r\n            {{ addr.city or \"\" }}<br/>\r\n            {{ addr.state or \"\" }}{% if addr.pincode %} - {{ addr.pincode }}{% endif %}<br/>\r\n            {{ addr.country or \"\" }}\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n                        <!--<tr>-->\r\n                        <!--    <td><strong>Contact Person:</strong> {{ doc.company_contact_person or \"\" }}</td>-->\r\n                        <!--</tr>-->\r\n                        <!--<tr>-->\r\n                        <!--    <td><strong>Contact No:</strong> {{ doc.company_contact_no or \"\" }}</td>-->\r\n                        <!--</tr>-->\r\n                        <tr>\r\n                            <td><strong>GST:</strong> {{ doc.company_gstin or \"\" }}</td>\r\n                        </tr>\r\n                    </table>\r\n                </td>\r\n\r\n                <!-- RIGHT: PO Info (20%) with its own inner table -->\r\n                <td style=\"width:30%; vertical-align:top; border:1px solid #000;\">\r\n                    <table style=\"width:100%;border-collapse:collapse; font-size:10px;\">\r\n                        <tr style = \"border-bottom:1px solid #000;\">\r\n                            <td style=\"border-right: 1px solid #000;\"><strong>Date</strong></td>\r\n                            <td style=\"\">{{ frappe.utils.formatdate(doc.transaction_date, 'dd-MMM-yyyy') }}</td>\r\n                        </tr>\r\n                        <tr style = \"border-bottom:1px solid #000;\">\r\n                            <td style=\"border-right: 1px solid #000;\"><strong>PO No</strong></td>\r\n                            <td style=\"\">{{ doc.name or \"\" }}</td>\r\n                        </tr>\r\n                        <tr style = \"border-bottom:1px solid #000;\">\r\n                            <td style=\"border-right: 1px solid #000;\"><strong>OC No</strong></td>\r\n                            <td style=\"\">{{ doc.custom_oc_no or \"\" }}</td>\r\n                        </tr>\r\n                        <tr style = \"border-bottom:1px solid #000;\">\r\n                            <td style=\"border-right: 1px solid #000;\"><strong>Dispatch Type</strong></td>\r\n                            <td style=\"\">{{ doc.tc_name or \"\" }}</td>\r\n                        </tr>\r\n                        <tr style=\"border-bottom:1px solid #000;\">\r\n    <td style=\"border-right: 1px solid #000;\"><strong>Contact Person</strong></td>\r\n    <td>\r\n        {% if doc.owner %}\r\n            {{ frappe.get_doc(\"User\", doc.owner).full_name or \"\" }}\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n\r\n                        <tr style=\"border-bottom:1px solid #000;\">\r\n    <td style=\"border-right: 1px solid #000;\"><strong>Contact No</strong></td>\r\n    <td>\r\n        {% if doc.owner %}\r\n            {{ frappe.get_doc(\"User\", doc.owner).mobile_no or \"\" }}\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n<tr>\r\n    <td style=\"border-right: 1px solid #000;\"><br><br></td>\r\n    <td>\r\n        <br><br>\r\n    </td>\r\n</tr>\r\n                    </table>\r\n                </td>\r\n            </tr>\r\n        </table>\r\n        <div style =\"text-align:center;background-color:#b6e1fb;\"><strong> PO DETAILS</strong></div>\r\n        \r\n        <!-- ITEM TABLE WITH IGST AUTO SWITCH -->\r\n        <!--<table style=\"width:100%; border-bottom:1px solid #000;border-top:1px solid #000;\">-->\r\n        <table class=\"items-table\" style=\"width:100%; border-bottom:1px solid #000;border-top:1px solid #000;\">\r\n            <thead>\r\n                <tr style=\"font-weight:bold; font-size:11px;\">\r\n                    <th width= \"5%\" style = \"border-right:1px solid #000;\"><strong>Sr. No.</strong></th>\r\n                    <th width= \"45%\" style = \"border-right:1px solid #000; text-align:center;\"><strong>Description</strong></th>\r\n                    <th width= \"10%\" style = \"border-right:1px solid #000;\"><strong>List Price</strong></th>\r\n                    <th width= \"10%\" style = \"border-right:1px solid #000;\"><strong>Discount Percent</strong></th>\r\n                    <th width= \"10%\" style = \"border-right:1px solid #000;\"><strong>Unit Rate</strong></th>\r\n                    <th width= \"8%\" style = \"border-right:1px solid #000;\"><strong>Qty</strong></th>\r\n                    <th width=\"7%\" style=\"border-right:1px solid #000;\"><strong>UOM</strong></th>\r\n                    <th width= \"10%\"><strong>Net Amount</strong></th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n    {% for row in doc.items %}\r\n        <tr style=\"font-size:10px; border-bottom:1px solid #000;\">\r\n            <td style=\"text-align:center; border-right:1px solid #000;\">{{ loop.index }}</td>\r\n            <td style=\"text-align:center;border-right:1px solid #000;\">{{ row.description }}</td>\r\n            <td style=\"text-align:center;border-right:1px solid #000; white-space: nowrap;\">{{ frappe.format(row.price_list_rate, {\"fieldtype\":\"Currency\", \"options\": doc.currency}) }}</td>\r\n            <td style=\"text-align:center;border-right:1px solid #000;\">{{ row.custom_discount_percent }}</td>\r\n            <td style=\"text-align:center;border-right:1px solid #000; white-space: nowrap;\">{{ frappe.format(row.rate, {\"fieldtype\":\"Currency\", \"options\": doc.currency}) }}</td>\r\n            <td style=\"text-align:center;border-right:1px solid #000;\">{{ row.qty }}</td>\r\n            <td style=\"text-align:center;border-right:1px solid #000;\">{{ row.uom }}</td>\r\n            <td style=\"text-align:center; white-space: nowrap;\">{{ frappe.format(row.amount, {\"fieldtype\":\"Currency\", \"options\": doc.currency}) }}</td>\r\n        </tr>\r\n    {% endfor %}\r\n</tbody>\r\n\r\n                <!--</table>-->\r\n                <!-- TOTAL ROW WITH QUANTITY ADDED -->\r\n            <tr style=\"font-weight:bold; font-size:11px; background-color:#b6e1fb;\">\r\n    <!-- Label spanning SR NO and Description -->\r\n    <td colspan=\"2\" style=\"border:1px solid #111; text-align:left; padding-left:5px;\">\r\n        Total\r\n    </td>\r\n\r\n    <!-- List Price column empty -->\r\n    <td style=\"border:1px solid #111;\"></td>\r\n\r\n    <!-- Discount Percent column empty -->\r\n    <td style=\"border:1px solid #111;\"></td>\r\n\r\n    <!-- Unit Rate column empty -->\r\n    <td style=\"border:1px solid #111;\"></td>\r\n\r\n    <!-- Total Quantity -->\r\n    <td style=\"border:1px solid #111; text-align:center;\">\r\n        {{ doc.items | sum(attribute='qty') }}\r\n    </td>\r\n    <td style=\"border:1px solid #111; text-align:center;\">\r\n    <!-- Unit total nahi hota -->\r\n</td>\r\n\r\n    <!-- Total Amount with â‚¹ above amount and center aligned -->\r\n    <td style=\"border:1px solid #111; text-align:center; vertical-align: middle;\">\r\n        <div style=\"line-height: 1; white-space: nowrap;\">\r\n    <span style=\"font-weight: bold; font-size: 10px;\">{{ frappe.format(doc.items | sum(attribute='net_amount'), {\"fieldtype\":\"Currency\", \"options\": doc.currency}) }}</span>\r\n</div>\r\n\r\n    </td>\r\n</tr>\r\n            <!--</tbody>-->\r\n        </table>\r\n        <table style =\"width:100%; border-collapse:collapse;\">\r\n            <tr style=\"border-bottom:1px solid #111; text-align:left;font-size:11px;\">\r\n                <td style=\"width:70%;\">\r\n  <table style=\"width:100%; border-collapse:collapse; font-size:10px;\">\r\n    <tr style=\"border-bottom:1px solid #000; background-color:##b6e1fb;\">\r\n        <td><strong>REMARK</strong></td>\r\n    </tr>\r\n\r\n    <!-- REMARKS -->\r\n    <tr>\r\n        <td style=\"vertical-align:top; white-space:pre-line; padding-bottom:6px;\">\r\n            {{ doc.custom_remarks or \"\" }}\r\n        </td>\r\n    </tr>\r\n\r\n    <!-- SEPARATOR -->\r\n    {% if doc.terms %}\r\n    <tr>\r\n        <td style=\"border-top:0px solid #000; height:4px;\"></td>\r\n    </tr>\r\n\r\n    <!-- DELIVERY TERMS -->\r\n    <tr>\r\n        <td style=\"vertical-align:bottom; font-size:10px;\">\r\n            <strong>Delivery Terms:</strong><br>\r\n            {{ doc.terms }}\r\n        </td>\r\n    </tr>\r\n    {% endif %}\r\n</table>\r\n\r\n</td>\r\n\r\n                <td style=\"width:30%;vertical-align:top; border:1px solid #000;\">\r\n                    <table style=\"width:100%;border-collapse:collapse; font-size:10px;\">\r\n{% set cgst_rate = doc.cgst_rate or (doc.items | map(attribute='cgst_rate') | max) or 0 %}\r\n{% set sgst_rate = doc.sgst_rate or (doc.items | map(attribute='sgst_rate') | max) or 0 %}\r\n{% set igst_rate = doc.igst_rate or (doc.items | map(attribute='igst_rate') | max) or 0 %}\r\n\r\n{% set total_cgst = doc.cgst or doc.items | sum(attribute='cgst_amount') or 0 %}\r\n{% set total_sgst = doc.sgst or doc.items | sum(attribute='sgst_amount') or 0 %}\r\n{% set total_igst = doc.igst or doc.items | sum(attribute='igst_amount') or 0 %}\r\n\r\n{% set is_igst = total_igst > 0 %}\r\n\r\n<tr style=\"border-bottom:1px solid #000;\">\r\n    <td style=\"border-right: 1px solid #000;\">\r\n        <strong>Subtotal</strong>\r\n    </td>\r\n    <td>{{ frappe.format(doc.total,{\"fieldtype\":\"Currency\",\"options\":doc.currency}) or \"\" }}</td>\r\n</tr>\r\n<!--{% for tax in doc.taxes %}-->\r\n<!--    {% if \"Freight and Forwarding Charges - JMT\" in tax.account_head %}-->\r\n<!--<tr style=\"border-bottom:1px solid #000;\">-->\r\n<!--    <td style=\"border-right: 1px solid #000;\">-->\r\n<!--        <strong>Freight</strong>-->\r\n<!--    </td>-->\r\n<!--    <td>-->\r\n<!--        {{ frappe.format(tax.tax_amount,{\"fieldtype\":\"Currency\",\"options\": doc.currency}) }}-->\r\n<!--    </td>-->\r\n<!--</tr>-->\r\n<!--    {% endif %}-->\r\n<!--{% endfor %}-->\r\n\r\n{% if not is_igst %}\r\n<tr style=\"border-bottom:1px solid #000;\">\r\n    <td style=\"border-right: 1px solid #000;\">\r\n        <strong>CGST ({{ cgst_rate }}%)</strong>\r\n    </td>\r\n    <td>\r\n        {% if total_cgst > 0 %}\r\n            {{ frappe.format(total_cgst, {\"fieldtype\": \"Currency\", \"options\": doc.currency}) }}\r\n        {% else %}\r\n            <!-- Ya zero chahiye toh yeh likh sakte ho: 0 -->\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n\r\n<tr style=\"border-bottom:1px solid #000;\">\r\n    <td style=\"border-right: 1px solid #000;\">\r\n        <strong>SGST ({{ sgst_rate }}%)</strong>\r\n    </td>\r\n    <td>\r\n        {% if total_sgst > 0 %}\r\n            {{ frappe.format(total_sgst, {\"fieldtype\": \"Currency\", \"options\": doc.currency}) }}\r\n        {% else %}\r\n            <!-- Ya zero chahiye toh yeh likh sakte ho: 0 -->\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n\r\n{% else %}\r\n<tr style=\"border-bottom:1px solid #000;\">\r\n    <td style=\"border-right: 1px solid #000;\">\r\n        <strong>IGST ({{ igst_rate }}%)</strong>\r\n    </td>\r\n    <td>\r\n        {% if total_igst > 0 %}\r\n            {{ frappe.format(total_igst, {\"fieldtype\": \"Currency\", \"options\": doc.currency}) }}\r\n        {% else %}\r\n            <!-- Ya zero chahiye toh yeh likh sakte ho: 0 -->\r\n        {% endif %}\r\n    </td>\r\n</tr>\r\n{% endif %}\r\n\r\n                        <!--<tr style = \"border-bottom:1px solid #000;\">-->\r\n                        <!--    <td style=\"border-right: 1px solid #000;\"><strong> Other Charges</strong></td>-->\r\n                        <!--    <td>{{ doc.dispatch_type or \"\" }}</td>-->\r\n                        <!--</tr>-->\r\n                        <tr style=\"border-bottom:1px solid #000;\">\r\n    {% set ns = namespace(disc_amt=0, disc_pct=0) %}\r\n\r\n    {% for i in doc.items %}\r\n        {% if i.discount_percentage and i.price_list_rate %}\r\n            {% set ns.disc_amt = ns.disc_amt +\r\n                ((i.price_list_rate * i.discount_percentage / 100) * i.qty)\r\n            %}\r\n            {% set ns.disc_pct = ns.disc_pct + i.discount_percentage %}\r\n        {% endif %}\r\n    {% endfor %}\r\n\r\n    <td style=\"border-right: 1px solid #000;\">\r\n        <strong>\r\n            Discount\r\n            {% if ns.disc_pct %}\r\n                ({{ ns.disc_pct }}%)\r\n            {% endif %}\r\n        </strong>\r\n    </td>\r\n\r\n    <td>\r\n        {{ frappe.format(\r\n            ns.disc_amt,\r\n            {\"fieldtype\":\"Currency\",\"options\":doc.currency}\r\n        ) }}\r\n    </td>\r\n</tr>\r\n\r\n                        <tr style=\"border-bottom:1px solid #000;\">\r\n    <td style=\"border-right: 1px solid #000;\"><strong>TCS</strong></td>\r\n    <td>\r\n        {{ frappe.format(doc.tcs_amount or 0, {\"fieldtype\":\"Currency\",\"options\":doc.currency}) }}\r\n    </td>\r\n</tr>\r\n\r\n<tr style=\"border-bottom:1px solid #000;\">\r\n    <td style=\"border-right: 1px solid #000;\"><strong>Round Off</strong></td>\r\n    <td>\r\n        {{ frappe.format(doc.rounding_adjustment or 0, {\"fieldtype\":\"Currency\",\"options\":doc.currency}) }}\r\n    </td>\r\n</tr>\r\n\r\n<tr>\r\n    <td style=\"border-right: 1px solid #000;\"><strong>Total Amt</strong></td>\r\n    <td>\r\n        {{ frappe.format(doc.rounded_total or 0, {\"fieldtype\":\"Currency\",\"options\":doc.currency}) }}\r\n    </td>\r\n</tr>\r\n\r\n                    </table>\r\n                </td>\r\n            </tr>\r\n        </table>\r\n        <!-- TOTAL IN WORDS -->\r\n        <div style=\"padding-left:3px;font-size:10px;background-color:#b6e1fb; border-bottom: 1px solid #000;\">\r\n            <span style=\"font-weight:bold;\">Total Amount (In words):</span>\r\n            <!--(frappe.utils.money_in_words(doc.grand_total))-->\r\n            {{ doc.in_words }}\r\n        </div>\r\n    </table>\r\n    \r\n    {% set approved_user = frappe.db.get_value(\r\n            \"Workflow Action\",\r\n            {\r\n                \"reference_name\": doc.name,\r\n                \"completed_by_role\": \"Purchase Manager\",\r\n                \"status\": \"Completed\"\r\n            },\r\n            \"completed_by\",\r\n            order_by=\"creation desc\"\r\n        ) %}\r\n\r\n<table style=\"width: 100%; font-family: 'Helvetica Neue', Arial, sans-serif; border-bottom: 1px solid #111;\">\r\n\r\n        <tr>\r\n            <td style = \"width :70% ;height:120px; position: relative;\">\r\n                <table style=\"width:100%; font-size:10px;position:absolute; bottom:5px;\">\r\n                <td style=\"width:50%; text-align:left;\">\r\n                    <strong>Created By:</strong>\r\n                    {% if doc.owner %}\r\n                        {{ frappe.get_doc(\"User\", doc.owner).full_name or doc.owner }}\r\n                    {% endif %}\r\n                </td>\r\n\r\n                <td style=\"width:50%; text-align:left;\">\r\n                    <strong>Approved By:</strong>\r\n                    {% if approved_user %}\r\n                        {{ frappe.get_doc(\"User\", approved_user).full_name or approved_user }}\r\n                    {% else %}\r\n                        NA\r\n                    {% endif %}\r\n                </td>\r\n            </tr>\r\n        </table>\r\n            </td>\r\n            <td style = \"width :30% ;border-left: 1px solid #000;height:120px;font-size:10px;text-align:center;\">\r\n                <span> <strong>For, JAIN MACHINE TOOLS &\r\n                        ELECTRICALS LLP</strong>\r\n                </span>\r\n                <p></p>\r\n                <p style = \"margin-top : 80px;\">\r\n                   <strong> AUTHORIZED SIGNATORY</strong>\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        \r\n    </table>\r\n    <table style=\"width: 100%; font-family: 'Helvetica Neue', Arial, sans-serif; border-bottom: 1px solid #111;\">\r\n            <tr>\r\n                <td style=\"width: 50%; vertical-align: top;\">\r\n                    <img src=\"/files/JMT FOOTER.png\" style=\"width: 100%; height: 60px;\">\r\n                </td>\r\n            </tr>\r\n    </table>\r\n    \r\n</div>\r\n<div class=\"bottom-fixed-footer\">\r\n    Page <span class=\"page\"></span> of <span class=\"topage\"></span>\r\n</div>\r\n\r\n\r\n{% if doc.workflow_state == \"Rejected\" or doc.workflow_state != \"Approved\" %}\r\n<div class=\"po-watermark\">\r\n    {% if doc.workflow_state != \"Approved\" and doc.workflow_state != \"Rejected\" %}\r\n        NOT APPROVED\r\n    {% elif doc.workflow_state == \"Rejected\" %}\r\n        REJECTED\r\n    {% endif %}\r\n</div>\r\n{% endif %}\r\n\r\n",
